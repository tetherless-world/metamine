    materialsmine:hasSelector "//Transmission_Electron_Microscopy" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:used ;
            owl:someValuesFrom materialsmine:ExposureTime ],
        [ a owl:Restriction ;
            owl:onProperty prov:used ;
            owl:someValuesFrom materialsmine:EmissionCurrent ],
        [ a owl:Restriction ;
            owl:onProperty prov:used ;
            owl:someValuesFrom materialsmine:WorkingDistance ],
        [ a owl:Restriction ;
            owl:onProperty prov:used ;
            owl:someValuesFrom materialsmine:AcceleratingVoltage ],
        [ a owl:Restriction ;
            owl:onProperty prov:used ;
            owl:someValuesFrom materialsmine:Magnification ],
        [ a owl:Restriction ;
            owl:onProperty prov:generated ;
            owl:someValuesFrom materialsmine:ElectricalProperty ],
        sio:Measuring ;
    skos:definition "Transmission electron microscopy for obtaining microstructure at nanoscale" .

